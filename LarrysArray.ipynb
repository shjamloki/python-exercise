   "source": [
    "from itertools import combinations as combo\n",
    "def larrysArray(A): # product of 3-cycles iff. even permutation\n",
    "    return \"NO\" if sum( A[i]>A[j] for i,j in combo(range(len(A)),2) )%2 else \"YES\"\n",
    "\n",
    "A = [9,8,10,7,3,5,2,11,4,6,1]\n",
    "larrysArray(A)"
   ]
