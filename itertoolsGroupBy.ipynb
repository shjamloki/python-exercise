   "source": [
    "import itertools \n",
    "key = []\n",
    "value =[]\n",
    "s = input()\n",
    "t = itertools.groupby(s)\n",
    "for i,j in t:\n",
    "    key.append(int(i))\n",
    "    value.append(len(list(j)))\n",
    "print(*zip(value,key))"
   ]
