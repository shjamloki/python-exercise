    "import string\n",
    "size =3\n",
    "a = string.ascii_lowercase\n",
    "L = []\n",
    "for i in range(size):\n",
    "    s = \"-\".join(a[i:size])\n",
    "    L.append((s[::-1]+s[1:]).center(4*size-3, \"-\"))\n",
    "print('\\n'.join(L[:0:-1]+L))"
